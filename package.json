{
  "name": "zvx",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://zerovirusexe.github.io/zvx",
  "dependencies": {
    "gh-pages": "^2.1.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build && npm run gh-build",
    "gh-build": "cd build && git add . && git commit -am. && git push -u origin master && cd ..",
    "gh-build-cd-in": "cd ./build",
    "gh-build-add": "git add .",
    "gh-build-commit": "git commit -am.",
    "gh-build-push": "git push -u origin master",
    "gh-build-cd-out": "cd ..",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
